{
  "manifest_version": 3,
  "name": "Facebook & Instagram Reels Volume Control",
  "version": "1.0",
  "description": "Adds a volume control slider to Facebook and Instagram Reels",
  "permissions": [
    "activeTab"
  ],
      "content_scripts": [
    {
      "matches": ["*://*.facebook.com/*", "*://*.instagram.com/*"],
      "js": ["content.js"],
      "css": ["styles.css"],
      "run_at": "document_end"
    }
  ],
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  }

}